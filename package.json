{
    "name": "Global-Consent-Manager",
    "version": "1.1.0",
    "description": "A browser extension for easier managing of GDPR consent cookies acros websites",
    "repository": "SociallyCompute/Global-Consent-Manager",
    "license": "MIT",
    "scripts": {
        "start": "web-ext run",
        "test": "eslint src/*.js"
    },
    "dependencies": {
        "eslint": "^5.3.0",
        "eslint-config-google": "^0.9.1",
        "web-ext": "^2.8.0"
    },
    "eslintConfig": {
        "extends": "google",
        "parserOptions": {
            "ecmaVersion": 2017
        },
        "rules": {
            "indent": [
                "error",
                4
            ],
            "max-len": [
                "error",
                {
                    "code": 120,
                    "ignoreStrings": true
                }
            ],
            "quotes": 0,
            "require-jsdoc": 0,
            "valid-jsdoc": 0
        }
    }
}
